{
  "name": "lofi_listener",
  "version": "1.0.0",
  "description": "Discord bot for listening only to Lofi Radio - ChilledCow",
  "main": "./build/index.js",
  "author": "Jonathan Zhao",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@types/node": "^14.14.10",
    "discord-akairo": "^8.1.0",
    "discord-ytdl-core": "^5.0.0",
    "discord.js": "^12.5.1",
    "file-type": "^16.0.1",
    "memjs": "^1.2.2",
    "mongoose": "^5.11.3",
    "typescript": "^4.1.2",
    "ytdl-core": "^4.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "standard": "^16.0.3"
  },
  "scripts": {
    "dev": "tsc && NODE_ENV=development node --optimize_for_size --max_old_space_size=460 --unhandled-rejections=strict -r dotenv/config build/main/index.js",
    "production": "tsc && NODE_ENV=production node --optimize_for_size --max_old_space_size=460 --unhandled-rejections=strict build/main/index.js",
    "listener": "tsc && node --optimize_for_size --max_old_space_size=460 --unhandled-rejections=strict build/listener/index.js",
    "devlistener": "tsc && node --optimize_for_size --max_old_space_size=460 --unhandled-rejections=strict -r dotenv/config build/listener/index.js",
    "rebuild": "rm -r build/* && tsc",
    "update": "yarn set version latest",
    "lint": "eslint src/**/*.ts",
    "db": "mongod -config /usr/local/etc/mongod.conf"
  }
}
